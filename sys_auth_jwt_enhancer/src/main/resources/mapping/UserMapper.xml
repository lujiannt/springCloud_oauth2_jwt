<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.enhancer.mapper.UserMapper">
    <select id="selectUserByName" parameterType="String" resultType="com.example.enhancer.entity.User">
        select * from user  where username = #{username}
    </select>
</mapper>